{
  "name": "xpx-score-explain-act",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "scripts": {
    "dev": "concurrently -n BACKEND,FRONTEND -c green,blue \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && ./.venv/bin/python -m uvicorn app.main:app --reload --port 8000",
    "dev:frontend": "cd frontend && npm run dev"
  }
}
